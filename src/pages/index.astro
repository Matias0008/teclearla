---
import CircleCursor from "@components/CircleCursor.astro";
import Contact from "@components/sections/Contact/Contact.astro";
import FAQ from "@components/sections/FAQ/FAQ.astro";
import Footer from "@components/sections/Footer/Footer.astro";
import Hero from "@components/sections/Hero/Hero.astro";
import Services from "@components/sections/Services/Services.astro";
import Webs from "@components/sections/Webs/Webs.astro";
import WorkDetail from "@components/sections/WorkDetail/WorkDetail.astro";
import Layout from "@layouts/Layout.astro";
---

<Layout
  title="Teclearla"
  description="Somos Teclearla, desarrollar tu web nunca fue tan facil. Desarrollamos tu página web a medida con un diseño único y adaptado a tus necesidades. Creamos sitios que generan futuro."
>
  <main class="flex flex-col w-full">
    <Hero />
    <WorkDetail />
    <Services />
    <Webs />
    <FAQ />
    <Contact />
    <Footer />
    <CircleCursor />
  </main>
</Layout>
